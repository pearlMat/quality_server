// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
}

model Contact {
  id       Int    @id @default(autoincrement())
  email    String @unique
  enquiry_title String?
  name     String
  phone    String
  message  String
}

model Applicant {
  id            Int    @id @default(autoincrement())
  title         String
  email         String @unique
  location      String
  first_name    String
  last_name     String
  phone         String
  experience    String
}

